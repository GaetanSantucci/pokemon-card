<%- include('../partials/header.ejs') %>

<main id="main">


  <% allPokemon.forEach(pokemon => { %>

  <a href="/pokemon/<%= pokemon.id %>" class="card">
    <img class="card-img" src="/images/<%= pokemon.image %>.png" alt="<%= pokemon.name %>_card">
    <h2 class="card-name"><%= pokemon.name %></h2>
    <div class="card-description">
      <div class="card-description-number">Card number :<%= pokemon.id %></div>
      <div class="card-description-type">Type: Electric</div>
    </div>
  </a>
  <% }) %>
</main>

<script src="/script/menuToggle.js" async></script>

<%- include('../partials/footer.ejs') %> 